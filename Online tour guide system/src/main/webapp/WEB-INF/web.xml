<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
         <servlet>
         	<servlet-name>tourguidelogin</servlet-name>
         	<servlet-class>com.tourlist.tourlistservlet</servlet-class>
         </servlet>
         <servlet-mapping>
         	<servlet-name>tourguidelogin</servlet-name>
         	<url-pattern>/log</url-pattern>
         </servlet-mapping>
         
         <servlet>
         	<servlet-name>tourguideregister</servlet-name>
         	<servlet-class>com.tourguide.tourguideregister</servlet-class>
         </servlet>
         <servlet-mapping>
         	<servlet-name>tourguideregister</servlet-name>
         	<url-pattern>/register</url-pattern>
         
         </servlet-mapping>
         
         <servlet>
         	<servlet-name>tourguideinsert</servlet-name>
         	<servlet-class>com.tourlist.tourguideinsert</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>tourguideinsert</servlet-name>
         	<url-pattern>/insert</url-pattern>
         </servlet-mapping>
         
         <servlet>
         	<servlet-name>updatelist</servlet-name>
         	<servlet-class>com.tourlist.updatelistservlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>updatelist</servlet-name>
         	<url-pattern>/update</url-pattern>
         </servlet-mapping>
         
          <servlet>
         	<servlet-name>deletelist</servlet-name>
         	<servlet-class>com.tourlist.deletelistservlet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>deletelist</servlet-name>
         	<url-pattern>/delete</url-pattern>
         </servlet-mapping>
         
         
</web-app>         